---
title: Articles by Tag
pageable: true
per_page: 12
layout: horace_default
comments: false
next: false
prev: false
---
<div class="outer">
    <div class="post-feed-title inner">Articles tagged '<%= tagname %>'</div>

    <div class="post-feed inner-wide">
      <% page_articles.each_with_index do |article, i|
           #next unless is_published(article)
        %>
        <%= partial "postbox", locals: { post: article } %>
      <% end %>
    </div>
</div>

<%= partial "pagination" %>
<%= partial "newsletter-box" %>
